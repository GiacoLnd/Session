{% extends 'base.html.twig' %}

{% block title %}Hello FormateurController!{% endblock %}

{% block body %}
<h1>Liste des formateurs :</h1>
{% for formateur in formateurs %}
    <a href="{{path('formateur_detail', {'id': formateur.getId()})}}">{{ formateur.prenom|capitalize }} {{ formateur.nom|capitalize }}</a>
{% endfor %}
{% endblock %}
