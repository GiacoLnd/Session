{% extends 'base.html.twig' %}

{% block title %}Détails d'un module{% endblock %}

{% block body %}
<h3 class="p-3">Détail d'un module de formation :</h3>
<p>Module : <strong>{{ formModule.getModuleName() }}</strong></p>
<p>Catégorie : <strong>{{ formModule.categorie.CategoryModuleName }}</strong></p>
<p>Sessions associées :
    {% for programme in formModule.programmes %}
    <strong>{{ programme.session.sessionName }}</strong>     
    {% endfor %}
</p>

{% endblock %}