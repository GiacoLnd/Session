{% extends 'base.html.twig' %}

{% block title %}Ajout de module{% endblock %}

{% block body %}
<h1>Ajoutez un module</h1>

{{form_start(formAddFormModule)}}
<div class="row"></div>
    <div class="col-12 text-center p-2">
        {{form_row(formAddFormModule.moduleName)}}
    </div>
    <div class="col-12 text-center p-2">
        {{form_row(formAddFormModule.categorie)}}
    </div>
    <div class="col-12 p-1">
        {{form_row(formAddFormModule.valider)}}
    </div>
</div>
{{form_end(formAddFormModule)}}

{% endblock %}