{% extends 'base.html.twig' %}

{% block title %}liste des stagiaires{% endblock %}

{% block body %}
<h1>Ajoutez un stagiaire</h1>

{{form_start(formEditStagiaire)}}

<div class="row">
    <div class="col-sm">
        {{form_row(formEditStagiaire.prenom)}}
    </div>
    <div class="col-sm">
        {{form_row(formEditStagiaire.nom)}}
    </div>
</div>
<div>
    <div class="col-sm">
        {{form_row(formEditStagiaire.ville)}}
    </div>
    <div class="col-sm">
        {{form_row(formEditStagiaire.dateNaissance)}}
    </div>
</div>
<div class="row">
    <div class="col-sm">
        {{form_row(formEditStagiaire.phone)}}
    </div>
    <div class="col-sm">
        {{form_row(formEditStagiaire.email)}}
    </div>
    <div class="col-sm">
        {{form_row(formEditStagiaire.sessions)}}
    </div>
</div>
<div class="w-100">
    {{form_row(formEditStagiaire.valider)}}
</div>
{{form_end(formEditStagiaire)}}

{% endblock %}
